{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "memorylink",
  "compatibility_date": "2025-12-13",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "memorylink-production",
      "database_id": "local-dev-db"
    }
  ]
}